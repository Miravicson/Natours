{
  "name": "natours",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/main.min.css -w --output-style",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css ",
    "compress:css": "node-sass css/style.prefix.css css/main.min.css --output-style compressed",
    "delete:css": "rm css/style.comp.css css/style.concat.css css/style.prefix.css",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css delete:css"
  },
  "keywords": [
    "Nature",
    "Colour"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "all": "0.0.0",
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2"
  },
  "dependencies": {
    "live-server": "^1.2.1"
  }
}
